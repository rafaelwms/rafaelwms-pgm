<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.unibratec.qualabastecer"
    android:versionCode="1"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.edu.unibratec.qualabastecer.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.edu.unibratec.qualabastecer.DetailsActivity"
            android:label="@string/title_activity_details" >
        </activity>
        <activity
            android:name="br.edu.unibratec.qualabastecer.EntradaActivity"
            android:label="@string/title_activity_entrada" >
        </activity>
    </application>

</manifest>
